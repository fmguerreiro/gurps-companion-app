(defn kebab-case [s]
    (when s
      (-> s
          str
          (clojure.string/replace #"([a-z])([A-Z])" "$1-$2")
          (clojure.string/replace #"([A-Z]+)([A-Z][a-z])" "$1-$2")
          clojure.string/lower-case
          (clojure.string/replace #"\s+" "-")
          (clojure.string/replace #"[-]+$" ""))))

(comment

  (->> parsed-spells
     (map #(merge % {:college (keyword (kebab-case (get % :college)))})))

  (->> spells
     (group-by :college))

(+ 1 2)

  (defn get-unique-maps [key coll]
    (->> coll
         (group-by #(get % key))
         (vals)
         (map first)))

  (->> parsed-spells
       (map :college)
       (map kebab-case)
       (map keyword)
       distinct)

  ;; TODO:
  ;; kebab -> keyword college
  ;; group by college
  ;; generate def for each college
  ;;
  ;; parse cast-time, duration, cost, prerequisites

  (->> parsed-spells
       (map #(str "(def ^:private '(symbol (keyword (kebab-case (get % :name)))) '" % ")"))
       (map cljs.reader/read-string))

  (+ 1 2))
